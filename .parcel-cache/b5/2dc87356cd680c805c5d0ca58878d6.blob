      <section id="Tasks" class="page-container">
        <h1>
          This is the Tasks page.
        </h1>
      </section>

      <section id="Dictionary" class="page-container">
        <h1>
          This is the Dictionary page.
        </h1>
      </section>

    <section id="Timer" class="page-container">
      <div class="aside">
        <hr class="solid">
        <nav class="sub_nav">
          <ul class="sub_nav_links">
            <li class="sub_nav_item"><a href="#Pomodoro" class="active">Pomodoro</a></li>
            <li class="sub_nav_item"><a href="#Short_Break">Short Break</a></li>
            <li class="sub_nav_item"><a href="#Long_Break">Long Break</a></li>
          </ul>
        </nav></div>
        


        <div id="pomodoro-app">
          <div id="container">
            <div id="timer">
              <div id="time">
                <span id="minutes">25</span>
                <span id="colon">:</span>
                <span id="seconds">00</span>
              </div>
              <div id="filler"></div>
            </div>
        
            <div id="buttons">
              <button id="work">Work</button>
              <button id="shortBreak">Short Break</button>
              <button id="longBreak">Long Break</button>
              <button id="stop">Stop</button>
            </div>
          </div>
        </div>



        <div id="Pomodoro" class="sub-page-container">
          <h2 class="time">25:00</h2>
          <button type="button" class="start_and_stop_button">Start</button>
        </div>

        <div id="Short_Break" class="sub-page-container" style="bd68a2e7fa44e851207b6eee835e7e27">
          <h2 class="time">5:00</h2>
          <button type="button" class="start_and_stop_button">Start</button>          <h2 class="start_and_stop_button" style="0ec597aea2300ef9935f39b6498afc4b">Stop</h2>
        </div>

        <div id="Long_Break" class="sub-page-container" style="1388dc46882fb6053f10e0f94cf42d85">
          <h2 class="time">15:00</h2>
          <button type="button" class="start_and_stop_button">Start</button>        </div>
    </section>

