        </div>
      </div>



      <section id="Tasks" class="page-container">
        <h1>
          This is the Tasks page.
        </h1>
      </section>

      <section id="Dictionary" class="page-container">
        <h1>
          This is the Dictionary page.
        </h1>
      </section>

    <section id="Timer" class="page-container">
      <div class="aside">
        <hr class="solid">
        <nav class="sub_nav">
          <ul class="sub_nav_links">
            <li class="sub_nav_item"><a href="#Pomodoro" class="active">Pomodoro</a></li>
            <li class="sub_nav_item"><a href="#Short_Break">Short Break</a></li>
            <li class="sub_nav_item"><a href="#Long_Break">Long Break</a></li>
          </ul>
        </nav></div>
        

        <!-- Pomodoro Timer -->
        <div id="Pomodoro" class="sub-page-container">
          <div class="row">
            <div class="col-5">
              <h4 id="minutes"></h4>
            </div>
            <div class="col-2">
              <h4>:</h4>
            </div>
            <div class="col-5">
              <h4 id="seconds"></h4>
            </div>
          </div>
        </div>

        <!-- Start and Stop Buttons -->

          <button type="button" class="start_and_stop_button">Start</button>

        <!-- Short Break -->
        <div id="Short_Break" class="sub-page-container" style="3f81e0c066248050d78325778561bac7">
          <h2 class="time">5:00</h2>
          <button type="button" class="start_and_stop_button">Start</button>          <h2 class="start_and_stop_button" style="39c6a75312e51a10054adf16a93c3a58">Stop</h2>
        </div>

        <!-- Long Break -->

        <div id="Long_Break" class="sub-page-container" style="2d1a834c0d3b6c93685e563966782dc8">
          <h2 class="time">15:00</h2>
          <button type="button" class="start_and_stop_button">Start</button>        </div>
    </section>

      <section id="Stopwatch" class="page-container">
        <h1>
          This is the Stopwatch page.
        </h1>
      </section>

      <section id="Listen" class="page-container">
        <h1>
          This is the Listen page.
        </h1>
      </section>

